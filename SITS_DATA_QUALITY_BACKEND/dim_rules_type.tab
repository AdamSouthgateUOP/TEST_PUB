create table DIM_RULES_TYPE
(
  rules_type_skey               NUMBER generated by default as identity (start with 10000000 increment by 50),
  rules_type_bkey               VARCHAR2(20),
  rules_type_description        VARCHAR2(200),
  rules_type_skey_1_description VARCHAR2(200),
  rules_type_skey_2_description VARCHAR2(200)
)
;
alter table DIM_RULES_TYPE
  add constraint DIM_DIMENSION_DIMENSION_SKEY primary key (RULES_TYPE_SKEY);

